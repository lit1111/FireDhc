                                game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
                                dropmoney(10000)
                            end
                        end
		            end
                
                    if chat == '$moneyclub' then 
                        for i,v in pairs(chr:GetChildren()) do
