                                v.CanCollide = false
                            end
                        end
                        while task.wait() do
                            if chr and chr:FindFirstChild('HumanoidRootPart') then
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-931.555115, 10.7708454, -481.936768) * CFrame.Angles(math.rad(-90),0,0)
                                game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
                                dropmoney(10000)
                            end
                        end
			        end
                    
                    if chat == '$moneycourt' then 
                        for i,v in pairs(chr:GetChildren()) do
                            if v:IsA('MeshPart') then
                                v.CanCollide = false
                            end
                        end
                        while task.wait() do
                            if chr and chr:FindFirstChild('HumanoidRootPart') then
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-931.555115, 10.7708454, -481.936768) * CFrame.Angles(math.rad(90),0,0)
                                game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
                                dropmoney(100)
                                game.Players.LocalPlayer.Character["[Money Gun]"]:Activate()
                            end
                        end
                    end

                    if chat == '$dropadmin' then 
                        for i,v in pairs(chr:GetChildren()) do
                            if v:IsA('MeshPart') then
                                v.CanCollide = false
                            end
                        end
                        while task.wait() do
                            if chr and chr:FindFirstChild('HumanoidRootPart') then
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-871.685181, -49.39785, -592.350891) * CFrame.Angles(math.rad(-90),0,0)
                                game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
                                dropmoney(10000)
                            end
                        end
                    end
                    
                    if chat == '$moneyadmin' then 
                        for i,v in pairs(chr:GetChildren()) do
                            if v:IsA('MeshPart') then
                                v.CanCollide = false
                            end
                        end
                        while task.wait() do
                            if chr and chr:FindFirstChild('HumanoidRootPart') then
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-871.685181, -49.39785, -592.350891) * CFrame.Angles(math.rad(90),0,0)
                                game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
                                dropmoney(100)
                                game.Players.LocalPlayer.Character["[Money Gun]"]:Activate()
                            end
                        end
                    end

    
		            if chat == '$dropclub' then 
                        for i,v in pairs(chr:GetChildren()) do
